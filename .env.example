# Server Configuration
PORT=3000
TMP_DIR_PREFIX=bucket-

# Database Configuration
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_HOST=localhost
DB_PORT=5432
DB_NAME=bucket
DB_SSL=false

# Embedding Service Configuration
EMBEDDING_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
DASHSCOPE_API_KEY=your_dashscope_api_key
EMBEDDING_MODEL=text-embedding-v3
EMBEDDING_DIMENSIONS=1536
MAX_EMBEDDINGS_PER_BATCH=10

# Video Processing Configuration
DEFAULT_FPS=30
MAX_DOWNLOAD_SIZE_MB=500
DOWNLOAD_TIMEOUT_MS=120000

# Upload Configuration
UPLOAD_TIMEOUT_MS=120000

# FFmpeg GPU Acceleration (NVIDIA CUDA)
# Set to true to enable NVIDIA GPU acceleration for video encoding
FFMPEG_GPU_ACCELERATION=false

# GPU Encoder Codec (h264_nvenc, hevc_nvenc, av1_nvenc)
# Only used when FFMPEG_GPU_ACCELERATION=true
FFMPEG_GPU_ENCODER=h264_nvenc

# GPU Encoding Preset (p1-p7, where p1=fastest, p7=slowest/best quality)
# Only used when FFMPEG_GPU_ACCELERATION=true
FFMPEG_GPU_PRESET=p4

# Target bitrate for GPU encoding (e.g., 5M, 10M, 20M)
# Only used when FFMPEG_GPU_ACCELERATION=true and re-encoding is needed
FFMPEG_GPU_BITRATE=5M

# Enable spatial adaptive quantization (improves quality)
# Only used when FFMPEG_GPU_ACCELERATION=true
FFMPEG_GPU_SPATIAL_AQ=true

# Enable temporal adaptive quantization (improves quality)
# Only used when FFMPEG_GPU_ACCELERATION=true
FFMPEG_GPU_TEMPORAL_AQ=true

# Rate control lookahead (10-20 frames recommended)
# Only used when FFMPEG_GPU_ACCELERATION=true
FFMPEG_GPU_RC_LOOKAHEAD=20
